@{
    ViewBag.Title = "Home Page";
}
<div class="container">
    <table class="table">
        <tr>
            <th>
                Title
            </th>
            <th>
                Description
            </th>
            <th>
                Created
            </th>
            <th>
                Link
            </th>
        </tr>

        @foreach (var link in ViewBag.AllLinks)
        {
            <tr>
                <td>
                    @link.Title
                </td>
                <td>
                    @link.Description
                </td>
                <td>
                    @link.Created
                </td>
                <td>
                    <a href="@link.Url">@link.ShortUrl</a>
                </td>
            </tr>
        }

    </table>
</div>